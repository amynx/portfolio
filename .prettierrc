{
    "$schema": "https://json.schemastore.org/prettierrc",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "quoteProps": "as-needed",
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto"
}